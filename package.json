{
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "NODE_ENV=production tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token=d4fabc7465e2",
    "release": "auto shipit --base-branch=main"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "t-design-system",
  "author": "Gaurav Shankar",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "module": "dist/new-project-name.esm.js",
  "size-limit": [
    {
      "path": "dist/new-project-name.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/new-project-name.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@size-limit/preset-small-lib": "^5.0.1",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addons": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@tailwindcss/postcss7-compat": "^2.2.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/styled-components": "^5.1.11",
    "auto": "^10.29.3",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.9.2",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^5.0.1",
    "styled-components": "^5.3.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0"
  },
  "repository": "https://github.com/gauravshankar/t-design-system.git",
  "private": false
}
